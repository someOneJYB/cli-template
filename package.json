{
  "name": "library",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx vitest --config vitest.config.mts",
    "prepare": "husky",
    "build": "tsup",
    "docs:api": "typedoc",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:generate": "npm run docs:api && npm run docs:build",
    "preview": "npm run build && npm run docs:preview",
    "release": "standard-version --skip.commit --skip.tag && npm run docs:generate",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "postrelease": "git push --follow-tags origin main",
    "precommit": "npm run test && lint-staged --config lint-staged.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-dom": "^6.8.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "standard-version": "^9.0.0",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.13",
    "typedoc-plugin-mermaid": "^1.12.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "commitlint": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "eslint-scope": "^8.4.0",
    "eslint-visitor-keys": "^4.2.1"
  },
  "description": ""
}
